package kr.co.ch02.sub1;

public class LgTV {

	public void powerOn() {
		System.out.println("LgTV powerOn...");
	}
	public void powerOff() {
		System.out.println("LgTV powerOff...");
	}
	public void soundUp() {
		System.out.println("LgTV soundUp...");
	}
	public void soundDown() {
		System.out.println("LgTV soundDown...");
	}
}
